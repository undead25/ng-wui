{
  "compilerOptions": {
    "target": "es5",
    "module": "es2015",
    "moduleResolution": "node",
    "sourceMap": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": ["es2015", "dom"],
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "outDir": ".build"
  },
  "exclude": [
    "node_modules"
  ],
  "angularCompilerOptions": {
    "genDir": ".aot",
    "skipMetadataEmit": true,
    "debug": false
  }
}