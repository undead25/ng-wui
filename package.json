{
  "name": "ng-wui",
  "version": "0.0.1-alpha.28",
  "description": "Angular 2 web ui components and directives with Google Material Design",
  "scripts": {
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --inline --port 3001",
    "clean": "rimraf dist",
    "start": "npm run server:dev",
    "build": "npm run clean && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:server": "browser-sync start --server dist/ --files dist/*.html --port 8888",
    "ngc": "ngc -p ./tsconfig-aot.json",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "karma start",
    "coverage": "browser-sync start --server coverage/html --port 7777"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/undead25/ng-wui.git"
  },
  "author": "undead25",
  "license": "MIT",
  "devDependencies": {
    "@angular/common": "^2.4.1",
    "@angular/compiler": "^2.4.1",
    "@angular/compiler-cli": "^2.4.1",
    "@angular/core": "^2.4.1",
    "@angular/forms": "^2.4.1",
    "@angular/http": "^2.4.1",
    "@angular/platform-browser": "^2.4.1",
    "@angular/platform-browser-dynamic": "^2.4.1",
    "@angular/platform-server": "^2.4.1",
    "@angular/router": "^3.2.0",
    "@types/jasmine": "^2.5.40",
    "@types/node": "^6.0.55",
    "angular2-template-loader": "^0.6.0",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "^2.2.4",
    "browser-sync": "^2.18.5",
    "codelyzer": "^2.0.0-beta.4",
    "compression-webpack-plugin": "^0.3.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.25.0",
    "istanbul-instrumenter-loader": "^1.2.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.1",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.1",
    "node-sass": "^4.1.1",
    "postcss-loader": "^1.2.1",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "^2.5.4",
    "rxjs": "^5.0.2",
    "sass-loader": "^4.0.2",
    "script-ext-html-webpack-plugin": "^1.3.2",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "ts-helpers": "^1.1.2",
    "tslint": "^4.0.2",
    "typescript": "^2.0.10",
    "webpack": "^2.2.0-rc.3",
    "webpack-dev-middleware": "^1.9.0",
    "webpack-dev-server": "^2.2.0-rc.0",
    "webpack-merge": "^2.0.0",
    "zone.js": "^0.7.4"
  }
}
