{
  "name": "ng-wui",
  "version": "0.0.1-alpha.31",
  "description": "Angular 2 web ui components and directives with Google Material Design",
  "scripts": {
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --port 3001",
    "clean": "rimraf dist",
    "start": "npm run server:dev",
    "build": "npm run clean && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:server": "browser-sync start --server dist/ --files dist/*.html --port 8888",
    "ngc": "ngc -p ./tsconfig-aot.json",
    "tslint": "tslint -c tslint.json 'src/**/*.ts'",
    "test": "karma start",
    "coverage": "browser-sync start --server coverage/html --port 7777"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/undead25/ng-wui.git"
  },
  "author": "undead25",
  "license": "MIT",
  "devDependencies": {
    "@angular/animations": "^4.0.1",
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/compiler-cli": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/platform-server": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@types/jasmine": "^2.5.38",
    "@types/node": "^7.0.8",
    "@types/webpack": "^2.2.11",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "awesome-typescript-loader": "^3.1.2",
    "browser-sync": "^2.18.5",
    "codelyzer": "^3.0.0-beta.4",
    "compression-webpack-plugin": "^0.3.2",
    "core-js": "^2.4.1",
    "css-loader": "^0.28.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^2.28.0",
    "istanbul-instrumenter-loader": "^2.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.2",
    "karma-remap-coverage": "^0.1.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^2.0.2",
    "rimraf": "^2.6.1",
    "rxjs": "^5.2.0",
    "sass-loader": "^6.0.3",
    "script-ext-html-webpack-plugin": "^1.7.1",
    "source-map-loader": "^0.2.0",
    "style-loader": "^0.16.1",
    "ts-helpers": "^1.1.2",
    "tslint": "^5.0.0",
    "typescript": "^2.2.2",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.5"
  },
